{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-command-name": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off",
        "duplicate-id-active": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]",
          "iframe[loading]"
        ]
      }
    ]
  }
}